{
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "no-unused-expressions": "error",
    "no-unused-labels": "error",
    "no-unused-vars": "error"
  },
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  }
}
