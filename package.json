{
  "name": "music-discord-bot",
  "version": "1.0.3",
  "description": "Discord music bot that allows play music from youtube",
  "main": "build/src/main.js",
  "author": "statik-coder",
  "license": "ISC",
  "scripts": {
    "prebuild": "rm -rf build",
    "build": "tsc",
    "start:production": "cross-env NODE_ENV=production pm2 start build/src/main.js --attach",
    "start:dev": "cross-env NODE_ENV=development nodemon",
    "start:once": "cross-env NODE_ENV=development ts-node ./src/main.ts"
  },
  "dependencies": {
    "@discordjs/opus": "github:discordjs/opus",
    "@discordjs/voice": "^0.9.0",
    "@types/node": "^17.0.24",
    "chalk": "4.1.2",
    "cross-env": "^7.0.3",
    "discord.js": "^13.1.0",
    "dotenv": "^10.0.0",
    "ffmpeg": "^0.0.4",
    "ffmpeg-static": "^5.0.0",
    "inversify": "^5.1.1",
    "libsodium-wrappers": "^0.7.10",
    "play-dl": "^1.9.4",
    "reflect-metadata": "^0.1.13",
    "typescript": "^4.8.4",
    "ytpl": "^2.2.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "eslint": "^7.32.0",
    "nodemon": "^2.0.12",
    "pm2": "^5.2.2",
    "ts-node": "^10.9.1"
  }
}
